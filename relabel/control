$title
$symmetry c1
$user-defined bonds    file=coord
$coord    file=coord
$optimize
 internal   off
 redundant  off
 cartesian  on
 global     off
 basis      off
$atoms
na 1-8                                                                         \
   basis =na def-SV(P)
$basis    file=basis
$uhfmo_alpha   file=alpha
$uhfmo_beta   file=beta
$uhf
$alpha shells
 a       1-45                                   ( 1 )
$beta shells
 a       1-44                                   ( 1 )
$scfiterlimit      100
$scfconv        7
$thize     0.10000000E-04
$thime        5
$scfdamp   start=1.000  step=0.050  min=0.100
$scfdump
$scfintunit
 unit=30       size=0        file=twoint
$scfdiis
$maxcor    500 MiB  per_core
$scforbitalshift  automatic=.1
$drvopt
   cartesian  on
   basis      off
   global     off
   hessian    on
   dipole     on
   nuclear polarizability
$interconversion  off
   qconv=1.d-7
   maxiter=25
$coordinateupdate
   dqmax=0.3
   interpolate  on
   statistics    5
$forceupdate
   ahlrichs numgeo=0  mingeo=3 maxgeo=4 modus=<g|dq> dynamic fail=0.3
   threig=0.005  reseig=0.005  thrbig=3.0  scale=1.00  damping=0.0
$forceinit on
   diag=default
$energy    file=energy
$grad    file=gradient
$forceapprox    file=forceapprox
$rundimensions
   natoms=8
   nbf(CAO)=80
   nbf(AO)=80
$last step     dscf
$restart  dscf  twoint
$ssquare from dscf
          1.400 (not to be modified here)
$charge from dscf
         -1.000 (not to be modified here)
$dipole from dscf
  x    -6.28935832561945    y    -6.73024183560653    z    -1.92675087824219    a.u.
   | dipole | =   23.9202369324  debye
$end
